# qpidclient non-admin access
acl allow <%= node['qpid']['client']['username'] %>@<%= node['qpid']['sasl']['realm'] %> all all

# qpidadmin application admin
acl allow <%= node['qpid']['admin']['username'] %>@<%= node['qpid']['sasl']['realm'] %> all all

# defaults
acl deny-log guest@<%= node['qpid']['sasl']['realm'] %> all all
acl deny all all
